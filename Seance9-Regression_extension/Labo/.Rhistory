# Groupe des maths
N1 <- 28
s1_carre <- 6^2
# Groupe des philosophes
N2 <- 19
s2_carre <- 3^2
# Calcul de l'erreur par étape
## Numérateur fraction 1
nf1 <- N1*s1_carre + N2*s2_carre
## denominateur fraction 1
df1 <- N1 + N2 - 2
f1 <- nf1/df1
## Numérateur et denominateur fraction 2
nf2 <- N1 + N2
df2 <- N1*N2
f2 <- nf2/df2
## Erreur-type
s12 <- sqrt(f1*f2)
s12
t_calcule <- diff_X1X2/s12
t_calcule
